console.log("=== Testing Array.from() - simple copy ===");
const arr1 = [1, 2, 3, 4, 5];
const copied = Array.from(arr1);
console.log("Original array length: " + arr1.length);
console.log("Copied array length: " + copied.length);
console.log("First element: " + copied[0]);
console.log("Last element: " + copied[4]);

console.log("");
console.log("=== Testing Array.from() with mapper function ===");
const arr2 = [1, 2, 3, 4, 5];
const doubled = Array.from(arr2, (x, i) => x * 2);
console.log("Original array length: " + arr2.length);
console.log("Doubled array length: " + doubled.length);
console.log("First doubled element (should be 2): " + doubled[0]);
console.log("Second doubled element (should be 4): " + doubled[1]);
console.log("Third doubled element (should be 6): " + doubled[2]);

console.log("");
console.log("=== Testing Array.from() with index-based mapper ===");
const arr3 = [10, 20, 30];
const withIndex = Array.from(arr3, (x, i) => x + i);
console.log("First element (10 + 0 = 10): " + withIndex[0]);
console.log("Second element (20 + 1 = 21): " + withIndex[1]);
console.log("Third element (30 + 2 = 32): " + withIndex[2]);

