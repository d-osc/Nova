// Test different patterns
console.log("Pattern 1: string literal + boolean literal");
console.log("test " + true);

console.log("Pattern 2: boolean literal + string literal");
console.log(true + " test");

console.log("Pattern 3: string var + boolean var");
const s = "test ";
const b = true;
console.log(s + b);

console.log("Pattern 4: boolean var + string var");
const b2 = false;
const s2 = " test";
console.log(b2 + s2);
