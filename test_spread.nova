const arr1 = [1, 2, 3];
const arr2 = [4, 5, 6];
const combined = [...arr1, ...arr2];

// Print array elements to verify
console.log(combined[0]);  // Should print 1
console.log(combined[1]);  // Should print 2
console.log(combined[2]);  // Should print 3
console.log(combined[3]);  // Should print 4
console.log(combined[4]);  // Should print 5
console.log(combined[5]);  // Should print 6
