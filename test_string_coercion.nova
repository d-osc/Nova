// Test file for string coercion fix
// This tests number-to-string and boolean-to-string conversion

// Test 1: Number concatenation
console.log("Number: " + 42);

// Test 2: Boolean concatenation
console.log("Boolean: " + true);
console.log("Boolean: " + false);

// Test 3: Template literals with numbers
console.log(`Number: ${42}`);

// Test 4: Template literals with booleans
console.log(`Boolean: ${true}`);
console.log(`Boolean: ${false}`);

// Test 5: Mixed operations
let num = 100;
let bool = true;
console.log("Value: " + num + ", Flag: " + bool);

// Test 6: Multiple concatenations
console.log("A" + 1 + "B" + 2 + "C");

// Test 7: Float numbers
console.log("Pi: " + 3.14159);
console.log(`E: ${2.71828}`);
