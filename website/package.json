{
  "name": "nova-website",
  "version": "1.4.0",
  "description": "Official website for Nova Runtime - TypeScript/JavaScript to Native Code Compiler",
  "homepage": "https://d-osc.github.io/Nova",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "prismjs": "^1.29.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-clean": "rimraf build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build-clean",
    "deploy": "gh-pages -d build",
    "deploy-cli": "set NODE_OPTIONS=--max-old-space-size=4096 && npx gh-pages -d build --dotfiles --remove \".DS_Store\" \"Thumbs.db\"",
    "deploy-alt": "set NODE_OPTIONS=--max-old-space-size=4096 && npx --yes gh-pages@latest -d build --dotfiles --remove \".DS_Store\" \"Thumbs.db\" --no-silent"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "rimraf": "^5.0.5"
  }
}
