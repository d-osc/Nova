console.log("");
console.log("=== Testing Array.from() - simple copy ===");
const arr1 = [1, 2, 3, 4, 5];
const copied = Array.from(arr1);
console.log("Original length: " + arr1.length);
console.log("Copied length: " + copied.length);
console.log("First element: " + copied[0]);
console.log("Last element: " + copied[4]);

console.log("");
console.log("=== Testing Array.from() with mapper ===");
const arr2 = [1, 2, 3];
const doubled = Array.from(arr2, (x, i) => x * 2);
console.log("Doubled: [" + doubled[0] + ", " + doubled[1] + ", " + doubled[2] + "]");

console.log("");
console.log("=== Testing Array.from() with index mapper ===");
const arr3 = [10, 20, 30];
const indexed = Array.from(arr3, (x, i) => x + i);
console.log("Indexed: [" + indexed[0] + ", " + indexed[1] + ", " + indexed[2] + "]");

